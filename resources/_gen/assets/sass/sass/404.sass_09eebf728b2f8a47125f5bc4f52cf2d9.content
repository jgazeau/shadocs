@font-face{font-family:"oswald";font-weight:700;font-display:block;font-style:normal;src:url("../fonts/oswald/oswald-v49-latin-700.woff2") format("woff2")}body{font-family:oswald !important;overflow-x:hidden !important;overflow-y:auto !important;color:#363636;height:100vh;width:100vw;margin:0}#content404{justify-content:center;flex-direction:column;align-items:center;text-align:center;display:flex;height:100vh;width:100vw}#content404>*{padding:.5rem;max-width:100%;margin:0}#content404 #errorCode404{filter:blur(0.04rem);position:relative;user-select:none;font-size:5rem;padding:0}#content404 #errorCode404 span{clip-path:polygon(10% 0%, 44% 0%, 70% 100%, 55% 100%)}#content404 #errorCode404::before,#content404 #errorCode404::after{content:attr(data-text);position:absolute;left:0;top:0}#content404 #errorCode404::before{clip-path:polygon(0% 0%, 11% 0%, 56% 100%, 0% 100%);animation:crack1 2s linear forwards}#content404 #errorCode404::after{clip-path:polygon(43% 0%, 100% 0%, 100% 100%, 69% 100%);animation:crack2 2s linear forwards}#content404 #title404,#content404 #footer404{font-size:1.375rem}#content404 #footer404{padding:.5rem calc(.75rem + .5rem);margin:.5rem;background:transparent;text-decoration:none;color:#00334d;position:relative;outline:none}#content404 #footer404::after,#content404 #footer404::before{transition:all 0.4s cubic-bezier(0.8, 0, 0, 0.2);transform:skewX(30deg);position:absolute;border-radius:0;height:100%;z-index:-1;content:"";width:50%}#content404 #footer404::before{background:linear-gradient(90deg, #7dd47d 0%, #94db94 100%);left:.5rem;top:-.5rem}#content404 #footer404::after{background:linear-gradient(90deg, #6c6 0%, #7dd47d 100%);right:.5rem;top:.5rem}#content404 #footer404:hover::before,#content404 #footer404:hover::after{transform:skewx(0deg);top:0}#content404 #footer404:hover::before{border-radius:0 .25rem .25rem 0;left:50%}#content404 #footer404:hover::after{border-radius:.25rem 0 0 .25rem;right:50%}@keyframes crack1{0%,95%{transform:translate(0)}100%{transform:translate(-2%, 2%)}}@keyframes crack2{0%,95%{transform:translate(0)}100%{transform:translate(2%, -2%)}}
